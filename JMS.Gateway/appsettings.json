{
  "Port": 8911,
  "UnLockKeyTimeout": 10000, //当微服务掉线后，lockkey有效期（单位：毫秒）
  "DataFolder": "./data", //数据存放目录
  "AllowIps": [ "127.0.0.1" ], //允许哪些ip成为微服务，为空表示允许所有ip

  "SSL": {  //SSL为可选项
    "Cert": "d:/test.pfx",
    "Password": "123456",
    "AcceptCertHash": [ "92BDE5009AB5D12A1E127131819E18D74557E308" ] //被接受的对方证书的hash值，空数组表示信任所有证书
  },

  "Cluster": { //配置集群里的裁判地址，不做集群此项可移除
    "Referee2": {
      "Address": "127.0.0.1",
      "Port": 8919
    }
  },

  "ShareFolder": "./shares",//共享文件目录

  "ServiceProviderAllocator": {
    "FullName": "JMS.ServiceProviderAllocator",
    "Assembly": "JMS.Gateway"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}